{
  "name": "@panenco/pwa-utils",
  "version": "1.0.1",
  "description": "PWA enabling utils for Panenco react projects",
  "author": "Panenco <info@panenco.com>",
  "homepage": "https://github.com/Panenco/panenco#readme",
  "license": "MIT",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Panenco/panenco.git"
  },
  "scripts": {
    "build": "yarn clear && rollup -c",
    "clear": "rimraf dist lib",
    "lint": "eslint src --ext=jsx,ts,tsx --fix",
    "prettier": "prettier '{src}/**/*.{js,jsx,ts,tsx}' --write",
    "pre-commit": "lint-staged",
    "prepublishOnly": "yarn build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@panenco/eslint-config-react": "^1.0.3",
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^1.27.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.25.3"
  },
  "dependencies": {
    "workbox-background-sync": "^5.1.3",
    "workbox-cacheable-response": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-window": "^5.1.3"
  }
}
