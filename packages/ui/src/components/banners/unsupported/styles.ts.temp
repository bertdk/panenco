.unsupportedModal {
  background: white;
  border: 1px solid #90a4ae;
  border-radius: 4px;
  padding: 24px;
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 50%;

  &Content {
  }

  &Actions {
    align-items: flex-end;
    display: flex;
    margin-top: 24px;
  }
}
