.newVersionModal {
  background: white;
  border: 1px solid #90a4ae;
  border-radius: 4px;
  bottom: 0;
  padding: 24px;
  position: fixed;

  &Title {
    margin-bottom: 8px;
  }

  &Actions {
    align-items: flex-end;
    justify-content: flex-end;
    display: flex;
    margin-top: 24px;

    & > button:first-child {
      margin-right: 24px;
    }
  }
}
